# Copy this file to .env and fill secrets for local development

# JWT secrets (change these before sharing)
ACCESS_TOKEN_SECRET=change_this_access_secret
REFRESH_TOKEN_SECRET=change_this_refresh_secret

# Server
PORT=5000
NODE_ENV=development

# Google OAuth (optional)
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-google-client-secret
MONGODB_URI=mongodb+srv://<username>:<password>@cluster0.mongodb.net/mail?retryWrites=true&w=majority
FE_URL=http://localhost:3000
